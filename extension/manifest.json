{
  "name": "Jisho Extension",
  "manifest_version": 2,
  "version": "0.1",
  "description": "Extends Jisho.org",
  "permissions": [
    "storage"
  ],
  "background": {
    "scripts": ["event-page.js"],
    "persistent": false
  },
  "content_scripts": [{
      "matches": ["*://jisho.org/kanji/radicals/"],
      "js": ["jisho-radicals.js"],
	  "css": ["jisho-radicals.css"]
  }]
}